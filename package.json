  "dependencies": {
    "axios": "^0.9.1",
    "babel-relay-plugin": "^0.9.3",
    "body-parser": "^1.15.0",
    "crypto": "0.0.3",
    "db-migrate": "^0.9.23",
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "graphiql": "^0.7.8",
    "graphql": "^0.7.0",
    "graphql-relay": "^0.4.3",
