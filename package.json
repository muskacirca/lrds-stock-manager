    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.25.0",
    "fs": "0.0.2",
    "gulp-graphql": "^0.2.0",
    "jest-cli": "^15.1.1",
    "jest-react-native": "^15.0.0",
    "json": "^9.0.3",
    "json-loader": "^0.5.4",
    "lodash": "^4.15.0",
    "moment": "^2.11.2",
    "node-sass": "^3.10.0",
