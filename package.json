{
  "name": "lrds-stock-manager",
  "version": "0.0.1",
  "description": "",
  "main": "website/build/server/server.js",
  "scripts": {
    "build": "npm run build-css && node ./node_modules/babel-cli/bin/babel.js src/server --out-dir build && webpack",
